{
  "extends": "react-app",
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "jsx-a11y/rule-name": 0,
    "jsx-a11y/href-no-hash": 0,
    "no-redeclare": 1,
    "consistent-return": 0,
    "eqeqeq": [
      1,
      "allow-null"
    ],
    "eol-last": 1,
    "one-var": [
      "error",
      {
        "var": "never",
        "let": "never",
        "const": "never"
      }
    ],
    "sort-vars": 0,
    "space-before-function-paren": [
      2,
      "always"
    ],
    "vars-on-top": 1,
    "no-unused-expressions": 1,
    "dot-notation": [
      1,
      {
        "allowPattern": "^[a-z]+(_[a-z]+)+$"
      }
    ],
    "camelcase": 1,
    "operator-linebreak": [
      2,
      "before"
    ],
    "no-else-return": 1,
    "no-useless-concat": 1,
    "default-case": 1,
    "no-loop-func": 0,
    "no-new": 1,
    "strict": [
      2,
      "global"
    ],
    "consistent-this": 0,
    "guard-for-in": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/require-render-return": 2,
    "comma-dangle": [
      1,
      "never"
    ],
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "require-jsdoc": 0,
    "valid-typeof": 2,
    "accessor-pairs": [
      2,
      {
        "getWithoutSet": true,
        "setWithoutGet": false
      }
    ],
    "array-callback-return": 2,
    "block-scoped-var": 0,
    "complexity": 0,
    "dot-location": [
      1,
      "property"
    ],
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 1,
    "no-div-regex": 2,
    "no-empty-function": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 0,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 1,
    "no-useless-call": 1,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "radix": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "yoda": 0,
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      1,
      "nofunc"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": [
      2,
      "always"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "id-match": 0,
    "key-spacing": 0,
    "keyword-spacing": 2,
    "linebreak-style": 0,
    "lines-around-comment": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-nested-callbacks": 0,
    "max-params": 0,
    "max-statements": 0,
    "new-cap": 1,
    "new-parens": 0,
    "newline-after-var": [
      1,
      "always"
    ],
    "no-array-constructor": 2,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "no-nested-ternary": 1,
    "no-negated-condition": 0,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-plusplus": 0,
    "no-whitespace-before-property": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": 0,
    "operator-assignment": 1,
    "padded-blocks": [
      2,
      "never"
    ],
    "quote-props": 0,
    "quotes": [
      2,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "sort-imports": 0,
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": [
      2,
      {
        "int32Hint": true
      }
    ],
    "space-unary-ops": 2,
    "spaced-comment": 1,
    "wrap-regex": 0,
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "constructor-super": 2,
    "generator-star-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "no-class-assign": 2,
    "no-confusing-arrow": 0,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-var": 0,
    "no-useless-constructor": 1,
    "object-shorthand": [
      2,
      "always"
    ],
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-reflect": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 2,
    "yield-star-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "no-useless-escape": 0
  }
}